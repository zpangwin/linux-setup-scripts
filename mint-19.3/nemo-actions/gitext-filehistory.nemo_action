[Nemo Action]
Name=GitExt File History
Comment=GitExt File History
Exec="/usr/share/nemo/actions/scripts/launch-gitext-filehistory.sh" %F
Dependencies=git;mono;/opt/GitExtensions/gitext.sh;/usr/share/nemo/actions/scripts/launch-gitext-commit.sh;
Icon-Name=git-ext-file-history
Selection=s
Extensions=nodirs;
Quote=double
Conditions=exec /usr/bin/is-git-dir %F
